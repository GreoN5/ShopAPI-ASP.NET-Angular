class AuthGuard {
    constructor(router) {
        this.router = router;
    }
    canActivate(route, state) {
        if (localStorage.getItem('token') !== null) {
            return true;
        }
        else {
            this.router.navigateByUrl('/home');
            return false;
        }
    }
}

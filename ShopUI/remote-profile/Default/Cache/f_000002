class AddUserComponent {
    constructor(adminService) {
        this.adminService = adminService;
    }
    ngOnInit() {
        this.adminService.createModelUser.reset();
    }
    submit() {
        this.adminService.addNewUser().subscribe(response => {
            this.adminService.createModelUser.reset();
        }, error => {
            console.log(error);
        });
    }
}

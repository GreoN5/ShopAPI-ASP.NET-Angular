class ShowRemoveProductComponent {
    constructor(adminService, router) {
        this.adminService = adminService;
        this.router = router;
    }
    ngOnInit() {
        this.showProducts();
    }
    showProducts() {
        this.adminService.getAllProducts().subscribe(data => {
            this.adminService.allProducts = data;
        }, error => {
            console.log(error);
        });
    }
    editProduct(productID) {
        this.router.navigateByUrl('admin/edit/' + productID);
    }
    deleteProduct(productName) {
        this.adminService.removeProduct(productName).subscribe(data => {
            for (let i = 0; i < this.adminService.allProducts.length; i++) {
                if (this.adminService.allProducts[i].name === productName) {
                    this.adminService.allProducts.splice(i, 1);
                    break;
                }
            }
        }, error => {
            console.log(error);
        });
    }
}
